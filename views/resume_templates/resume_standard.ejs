<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%= resumeData.name %> - Resume</title>
    <style>
        .resume-template.olivia_wilson {
            font-family: 'Arial', sans-serif;
            line-height: 1.5;
            max-width: 800px;
            margin: 0 auto;
            padding: 20px;
            color: #1f2937;
            background: #fff;
        }
        .resume-template.olivia_wilson h1 {
            margin-bottom: 0;
            font-size: 2rem;
            font-weight: bold;
        }
        .resume-template.olivia_wilson h2 {
            margin-top: 5px;
            font-size: 1.1rem;
            font-weight: normal;
            color: #4d44db;
        }
        .resume-template.olivia_wilson h3 {
            font-size: 1.3rem;
            margin-bottom: 10px;
            border-bottom: 1px solid #ddd;
            padding-bottom: 3px;
            font-weight: bold;
        }
        .resume-template.olivia_wilson .header-section {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 20px;
        }
        .resume-template.olivia_wilson .contact-info {
            margin-bottom: 20px;
        }
        .resume-template.olivia_wilson .education-item,
        .resume-template.olivia_wilson .experience-item,
        .resume-template.olivia_wilson .project-item {
            margin-bottom: 15px;
        }
        .resume-template.olivia_wilson .school,
        .resume-template.olivia_wilson .company,
        .resume-template.olivia_wilson .project-name {
            font-weight: bold;
        }
        .resume-template.olivia_wilson .date {
            font-style: italic;
            color: #666;
        }
        .resume-template.olivia_wilson ul {
            margin-top: 5px;
            padding-left: 20px;
        }
        .resume-template.olivia_wilson .skills-section {
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
        }
        .resume-template.olivia_wilson .skill-category {
            flex: 1;
            min-width: 200px;
        }
        .resume-template.olivia_wilson .profile-image {
            width: 60px;
            height: 60px;
            border-radius: 50%;
            object-fit: cover;
            border: 2px solid #4d44db;
        }
        .resume-template.olivia_wilson .section {
            margin-bottom: 25px;
        }
    </style>
</head>
<body>
    <div class="resume-template olivia_wilson">

        <!-- Header -->
        <div class="header-section">
            <div>
                <h1><%= resumeData.name %></h1>
                <% if (resumeData.title) { %>
                    <h2><%= resumeData.title %></h2>
                <% } %>
            </div>
            <% if (resumeData.profileImage) { %>
                <img src="<%= resumeData.profileImage %>" alt="Profile Image" class="profile-image">
            <% } %>
        </div>

        <!-- Contact Info -->
        <% if (resumeData.email || resumeData.phone) { %>
            <div class="contact-info">
                <% if (resumeData.email) { %>
                    <p><%= resumeData.email %></p>
                <% } %>
                <% if (resumeData.phone) { %>
                    <p><%= resumeData.phone %></p>
                <% } %>
            </div>
        <% } %>

        <!-- Education -->
        <% if (resumeData.education && resumeData.education.length > 0) { %>
            <div class="section">
                <h3>Education</h3>
                <% resumeData.education.forEach(edu => { %>
                    <div class="education-item">
                        <% if (edu.school) { %><p class="school"><%= edu.school %></p><% } %>
                        <% if (edu.degree) { %><p><%= edu.degree %></p><% } %>
                        <% if (edu.major) { %><p><%= edu.major %></p><% } %>
                        <% if (edu.date) { %><p class="date"><%= edu.date %></p><% } %>
                    </div>
                <% }); %>
            </div>
        <% } %>

        <!-- Experience -->
        <% if (resumeData.experience && resumeData.experience.length > 0) { %>
            <div class="section">
                <h3>Work Experience</h3>
                <% resumeData.experience.forEach(exp => { %>
                    <div class="experience-item">
                        <% if (exp.date) { %><p class="date"><%= exp.date %></p><% } %>
                        <% if (exp.company) { %><p class="company"><%= exp.company %></p><% } %>
                        <% if (exp.position) { %><p><%= exp.position %></p><% } %>
                        <% if (exp.responsibilities && exp.responsibilities.length > 0) { %>
                            <ul>
                                <% exp.responsibilities.forEach(responsibility => { %>
                                    <% if (responsibility) { %><li><%= responsibility %></li><% } %>
                                <% }); %>
                            </ul>
                        <% } %>
                    </div>
                <% }); %>
            </div>
        <% } %>

        <!-- Projects -->
        <% if (resumeData.projects && resumeData.projects.length > 0) { %>
            <div class="section">
                <h3>Projects</h3>
                <% resumeData.projects.forEach(project => { %>
                    <div class="project-item">
                        <% if (project.date) { %><p class="date"><%= project.date %></p><% } %>
                        <% if (project.name) { %><p class="project-name"><%= project.name %></p><% } %>
                        <% if (project.technologies) { %><p><%= project.technologies %></p><% } %>
                        <% if (project.details && project.details.length > 0) { %>
                            <ul>
                                <% project.details.forEach(detail => { %>
                                    <% if (detail) { %><li><%= detail %></li><% } %>
                                <% }); %>
                            </ul>
                        <% } %>
                        <% if (project.link) { %>
                            <p><a href="<%= project.link %>">View Project</a></p>
                        <% } %>
                    </div>
                <% }); %>
            </div>
        <% } %>

        <!-- Skills -->
        <% if (resumeData.skills && Object.keys(resumeData.skills).length > 0) { %>
            <div class="section">
                <h3>Skills</h3>
                <div class="skills-section">
                    <% Object.entries(resumeData.skills).forEach(([category, skills]) => { %>
                        <% if (skills && skills.length > 0) { %>
                            <div class="skill-category">
                                <h4><%= category %></h4>
                                <ul>
                                    <% skills.forEach(skill => { %>
                                        <% if (skill) { %><li><%= skill %></li><% } %>
                                    <% }); %>
                                </ul>
                            </div>
                        <% } %>
                    <% }); %>
                </div>
            </div>
        <% } %>

        <!-- Achievements -->
        <% if (resumeData.achievements && resumeData.achievements.length > 0) { %>
            <div class="section">
                <h3>Achievements</h3>
                <ul>
                    <% resumeData.achievements.forEach(achievement => { %>
                        <% if (achievement) { %><li><%= achievement %></li><% } %>
                    <% }); %>
                </ul>
            </div>
        <% } %>
    </div>
</body>
</html>
